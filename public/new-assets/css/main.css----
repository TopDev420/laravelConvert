@-webkit-keyframes animScaleUp {
    from {
        opacity: 0;
        -webkit-transform: scale3d(0, 0, 1);
        transform: scale3d(0, 0, 1)
    }
    to {
        opacity: 1;
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1)
    }
}

@keyframes animScaleUp {
    from {
        opacity: 0;
        -webkit-transform: scale3d(0, 0, 1);
        transform: scale3d(0, 0, 1)
    }
    to {
        opacity: 1;
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1)
    }
}

@-webkit-keyframes animFade {
    from {
        opacity: 0
    }
    to {
        opacity: 1
    }
}

@keyframes animFade {
    from {
        opacity: 0
    }
    to {
        opacity: 1
    }
}


/* blog page css */

.custom-list-item-date {
    display: table-cell;
    vertical-align: top;
    width: 80px;
    padding-right: 2%;
}

.custom-item-date {
    display: block;
    border-radius: 50%;
    border: 3px solid #ececec;
    text-transform: uppercase;
    text-align: center;
    padding-top: 12px;
    width: 65px;
    height: 65px;
}

.custom-item-month {
    font-weight: 800;
    color: #26282b;
    font-size: 24px;
    display: block;
    line-height: 1;
    text-transform: uppercase;
    text-align: center;
}

.custom-item-year {
    text-transform: uppercase;
    text-align: center;
    font-family: "Proxima Nova", Helvetica, Arial, sans-serif;
    display: block;
    line-height: 1;
    color: #414141;
    letter-spacing: .5px;
    font-size: 11px;
}

.custom-item-content {
    display: table-cell;
    vertical-align: top;
}

.post-header h2 {
    letter-spacing: 2px;
    text-transform: uppercase;
    margin-bottom: 0;
    margin-top: 5px;
    font-size: 24px;
    font-weight: 700;
    line-height: 34px;
    color: #26282b;
}

.post-header h2 a {
    color: #26282b;
    text-decoration: none;
}

p.custom-item-author-topic a {
    font-weight: 600;
}

.custom-item-text p {
    color: #797f85;
    margin: 0;
    font-family: "Proxima Nova", Helvetica, Arial, sans-serif;
    font-size: 16px;
    line-height: 24px;
}

.custom-item-content a.more-link:hover {
    color: #be1413;
}

.custom-item-content a.more-link {
    color: #fd000d;
    padding: 0;
    font-size: 12px;
    letter-spacing: 1.8px;
    text-transform: uppercase;
    position: relative;
    display: inline-block;
    border-style: solid;
    border-width: 0;
    line-height: 1.5;
    text-align: left;
    text-decoration: none;
    cursor: pointer;
    -webkit-appearance: none;
    -webkit-transition-duration: .5s;
    transition-duration: .5s;
    -webkit-transition-property: background-color, color;
    transition-property: background-color, color;
    font-weight: 700;
}

.block-right h3 {
    margin: 12px 0;
    font-size: 12px;
    letter-spacing: 2px;
    text-transform: uppercase;
    font-family: 'FuturaLT', sans-serif;
    font-weight: 700;
    line-height: 1.1;
    color: #26282b;
    padding: 15px 25px;
}

.block-right .widget-module ul {
    margin: 0;
    padding: 0;
    list-style: none;
    display: block;
}

.block-right .widget-module ul li {
    border-top: 1px solid #e3e3e3;
}

.block-right .widget-module ul li a {
    -webkit-transition: background-color .3s ease-in;
    transition: background-color .3s ease-in;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: block;
    color: #797f85;
    padding: 15px 40px 15px 25px;
    font-weight: 600;
    position: relative;
    text-decoration: none;
}

.block-right {
    -webkit-box-shadow: 0 1px 3px rgba(0, 0, 0, .4);
    box-shadow: 0 1px 3px rgba(0, 0, 0, .4);
    margin-bottom: 24px;
    background: #fff;
    overflow: hidden;
}

.block-right .widget-module .filter-link-count {
    display: none;
}

.block-right .widget-module ul li a:hover {
    color: #797f85;
    background-color: #f3f3f3;
    text-decoration: none;
}

.block-right .widget-module ul li.recent a::after {
    position: absolute;
    top: 20px;
    color: #fd000d;
    content: "\f054";
    font-size: 14px;
    right: 15px;
    display: inline-block;
    font-family: 'FontAwesome';
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    text-rendering: auto;
    -webkit-transform: translate(0, 0);
    -ms-transform: translate(0, 0);
    transform: translate(0, 0);
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.news-leater h3 {
    margin: 0;
    font-size: 12px;
    letter-spacing: 2px;
    text-transform: uppercase;
    font-family: 'FuturaLT', sans-serif;
    font-weight: 700;
    line-height: 1.1;
    color: #26282b;
    padding: 15px 25px;
}

.input-blog {
    border-top: 1px solid #e3e3e3;
    padding: 15px 25px;
    margin-top: 15px;
    position: relative;
}

.submit-mail {
    position: absolute;
    right: 25px;
    top: 28px;
    color: #fd000d;
}